<m:mtd>
<m:mn>0</m:mn>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mn>0</m:mn>
</m:mtd>
<m:mtd>
<m:mn>1</m:mn>
</m:mtd>
</m:mtr>
<m:mtr>
<m:mtd>
<m:mn>1</m:mn>
</m:mtd>
<m:mtd>
<m:mn>1</m:mn>
</m:mtd>
</m:mtr>
</m:mtable>
<m:mo>]</m:mo>
</m:math></equation>
        <equation id="element-513"><m:math>
<m:msub>
<m:mi>F</m:mi>
<m:mn>2</m:mn>
</m:msub>
<m:mo>=</m:mo>
<m:mo>[</m:mo>
<m:mtable>
<m:mtr>
<m:mtd><m:mn>1</m:mn></m:mtd>
