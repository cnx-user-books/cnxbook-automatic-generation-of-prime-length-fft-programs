                        </m:mrow>
                      </m:msubsup>
                    </m:mtd>
</m:mtr>
<m:mtr>
<m:mtd/>
                    <m:mtd>
                      <m:mo>⋮</m:mo>
                    </m:mtd>
<m:mtd/>
</m:mtr>
<m:mtr>
                    <m:mtd>
                      <m:msubsup>
                        <m:mi>i</m:mi>
                        <m:mrow>
                          <m:mn>2</m:mn>
                          <m:mi>n</m:mi>
